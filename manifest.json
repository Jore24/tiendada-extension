{
  "manifest_version": 3,
  "name": "Tiendada Extension",
  "version": "1.0",
  "description": "QA Extension",
  "icons": {
    "128": "image.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
